{
  "expo": {
    "name": "LinguaQuest",
    "slug": "lingua-quest",
    // not used, see eas.json (done remotely)
    // "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo3.2.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#83389f",
    "splash": {
      "image": "./assets/logo3.2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marc.torsoc.linguaquest"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo3.2.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.marc.torsoc.linguaquest",
      "permissions": [
        "com.google.android.gms.permission.AD_ID"
      ]
    },
    "androidStatusBar": {
      "hidden": true
    },
    "web": {
      "favicon": "./assets/logo3.2.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "f7d8172d-4553-4ea0-ae23-529b7d720226"
      },
      "expo-navigation-bar": {
        "position": "absolute",
        "visibility": "hidden",
        "behavior": "inset-swipe",
        "backgroundColor": "#00000080"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "marc.torsoc",
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "visibility": "hidden",
          "behavior": "inset-swipe",
          "backgroundColor": "#00000080"
        }
      ],
      "expo-router"
    ],
    "scheme": "lingua-quest"
  }
}
